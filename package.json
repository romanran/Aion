{
  "name": "tdsoft_aion",
  "version": "1.0.0",
  "description": "base dev build tool",
  "watch": {
    "build:ts": {
      "patterns": "TS",
      "extensions": "js,ts"
    },
    "build:less": {
      "patterns": "LESS",
      "extensions": "less,css"
    }
  },
  "scripts": {
    "test": "test",
    "lessc": "lessc",
    "ts": "typescript",
    "postcss": "postcss",
    "start": "npm run bs:start & npm run watch",
    "bs:start": "browser-sync start -c=\"bin/bs-config.js\" ",
    "bs:stream": "browser-sync reload",
    "build:less": "node bin/build-less",
    "build:ts": "node bin/build-ts",
    "watch": "npm-watch"
  },
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/RomanRanniew/tdsoft_aion.git"
  },
  "author": "TDSoft",
  "license": "BSD-Protection",
  "homepage": "https://bitbucket.org/RomanRanniew/tdsoft_aion#readme",
  "devDependencies": {
    "beepbeep": "^1.2.0",
    "browser-sync": "^2.18.2",
    "less": "^2.7.1",
    "less-plugin-autoprefix": "^1.5.1",
    "less-plugin-clean-css": "^1.5.1",
    "lodash": "^4.17.2",
    "minimatch": "^3.0.3",
    "node-notifier": "^4.6.1",
    "path": "^0.12.7",
    "postcss": "^5.2.6",
    "postcss-size": "^1.0.0",
    "postcss-sprites": "^4.1.0",
    "strip-debug": "^1.1.1",
    "svgmin": "^0.1.0",
    "svgstore": "^1.1.0",
    "tslint": "^4.0.2",
    "typescript": "^2.0.10"
  },
  "dependencies": {
    "hammerjs": "^2.0.8",
    "jquery": "^3.1.1",
    "jquery-hammerjs": "^2.0.0",
    "jquery-match-height": "^0.7.0",
    "malihu-custom-scrollbar-plugin": "^3.1.5",
    "node-cmd": "^1.1.1",
    "npm-watch": "^0.1.6",
    "postcss-cli": "^2.6.0",
    "slick": "^1.12.2"
  }
}
