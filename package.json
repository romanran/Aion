{
  "name": "tdsoft_aion",
  "version": "1.0.0",
  "description": "base dev build tool",
  "private": true,
  "watch": {
    "build:ts": {
      "patterns": [
        "TS"
      ],
      "extensions": "js,ts"
    },
    "build:less": {
      "patterns": [
        "LESS"
      ],
      "extensions": "less,css"
    },
    "restart": {
      "patterns": [
        "bin",
        "./package.json",
        "./tsconfig.json"
      ],
      "extensions": "js,json"
    }
  },
  "scripts": {
    "postinstall": "node bin bin/postinstall",
    "test": "test",
    "browserify": "browserify",
    "para": "parallelshell",
    "svgstore": "svgstore",
    "lessc": "lessc",
    "uglifyjs": "uglify-js",
    "browser-sync": "browser-sync",
    "tsc": "tsc",
    "tscg": "tscg",
    "tslint": "tslint",
    "postcss": "postcss",
    "notify": "node-notifier",
    "start": "npm run para \"npm run bs:start\" \"npm run watch\"",
    "restart": "npm start",
    "bs:start": "browser-sync start --config bin/bs-config.js ",
    "bs:stream": "browser-sync reload",
    "build:less": "node bin/build-less",
    "prebuild:ts": "npm run lint:ts",
    "lint:ts": "node bin/lint-ts",
    "build:ts": "tsc --allowJs | node bin/build-ts",
    "build:jslibs": "browserify JSLIBS/main.js > ../dist/js/libs.js && uglifyjs ../dist/js/libs.js > ../dist/js/libs.min.js",
    "watch": "npm run para \"npm run watch:less\" \"npm run watch:ts\" ",
    "watch:ts": "chokidar \"TS/**/*.ts\" -c \"npm run build:ts\"",
    "watch:less": "npm run build:less",
    "watch:svg": "chokidar \"SVG/**/*.*\" -c \"node bin/build-svg\" ",
    "watch:images": "chokidar \"IMG/**/*.*\" -i \"IMG/sprites/**\" -c \"node bin/compress-img\"  "
  },
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/RomanRanniew/tdsoft_aion.git"
  },
  "author": "TDSoft",
  "license": "BSD-Protection",
  "homepage": "https://bitbucket.org/RomanRanniew/tdsoft_aion#readme",
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "beepbeep": "^1.2.0",
    "browser-sync": "^2.18.2",
    "browserify": "^13.1.1",
    "chokidar-cli": "^1.2.0",
    "cli": "^1.0.1",
    "colors": "^1.1.2",
    "fs": "0.0.1-security",
    "glob": "^7.1.1",
    "imagemin": "^5.2.2",
    "imagemin-mozjpeg": "^6.0.0",
    "imagemin-pngquant": "^5.0.0",
    "less": "^2.7.1",
    "less-plugin-autoprefix": "^1.5.1",
    "less-plugin-clean-css": "^1.5.1",
    "less-plugin-functions": "0.0.2",
    "less-plugin-glob": "^1.1.1",
    "minimatch": "^3.0.3",
    "mkdirp": "^0.5.1",
    "node-cmd": "^1.1.1",
    "node-notifier": "^4.6.1",
    "node-run-cmd": "^1.0.1",
    "parallelshell": "^2.0.0",
    "path": "^0.12.7",
    "postcss": "^5.2.6",
    "postcss-cli": "^2.6.0",
    "postcss-size": "^1.0.0",
    "postcss-sprites": "^4.1.1",
    "strip-debug": "^1.1.1",
    "stripcolorcodes": "^0.1.0",
    "svgstore": "^2.0.2",
    "tslint": "^4.0.2",
    "typescript": "^2.0.10",
    "typescript-with-globs": "^0.1.4",
    "uglify-js": "^2.7.5",
    "util": "^0.10.3"
  },
  "dependencies": {
    "@types/lodash": "^4.14.44",
    "hammerjs": "^2.0.8",
    "jquery": "^3.1.1",
    "jquery-hammerjs": "^2.0.0",
    "jquery-match-height": "^0.7.0",
    "lodash": "^4.17.3",
    "malihu-custom-scrollbar-plugin": "^3.1.5",
    "slick": "^1.12.2"
  }
}
