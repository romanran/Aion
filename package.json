    "babel": "^6.5.2",
    "babel-cli": "^6.18.0",
    "babel-preset-es2015": "^6.18.0",
    "beepbeep": "^1.2.0",
    "browser-sync": "^2.18.2",
    "browserify": "^13.1.1",
    "browserify-shim": "^3.8.12",
    "chokidar": "^1.6.1",
    "cli": "^1.0.1",
    "colors": "^1.1.2",
    "fs": "0.0.1-security",
    "glob": "^7.1.1",
    "imagemin": "^5.2.2",
    "imagemin-mozjpeg": "^6.0.0",
    "imagemin-pngquant": "^5.0.0",
    "less": "^2.7.1",
    "less-plugin-autoprefix": "^1.5.1",
    "less-plugin-clean-css": "^1.5.1",
    "less-plugin-functions": "0.0.2",
    "less-plugin-glob": "^1.1.1",
    "minimatch": "^3.0.3",
    "mkdirp": "^0.5.1",
    "node-notifier": "^4.6.1",
    "parallelshell": "^2.0.0",
    "path": "^0.12.7",
    "postcss": "^5.2.6",
    "postcss-size": "^1.0.0",
    "postcss-sprites": "^4.1.1",
    "strip-debug": "^1.1.1",
    "stripcolorcodes": "^0.1.0",
    "svgmin": "^0.1.0",
    "svgo": "^0.7.1",
    "svgstore": "^2.0.2",
    "uglify-js": "^2.7.5",
    "util": "^0.10.3"
  },
