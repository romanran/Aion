{
  "name": "tdsoft_aion",
  "version": "1.0.1",
  "description": "base dev build tool",
  "private": true,
  "scripts": {
    "postinstall": "node bin/postinstall",
    "test": "test",
    "chokidar": "chokidar",
    "browserify": "browserify",
    "para": "parallelshell",
    "uglifyjs": "uglify-js",
    "postcss": "postcss",
    "notify": "node-notifier",
    "start": "npm run para \"npm run watch:jslibs\" \"node bin/main \"",
    "restart": "npm start",
    "watch:jslibs": "chokidar \"../src/JSLIBS/*.js\" -c \"build:jslibs\"",
    "build:jslibs": "browserify \"main.js\" --standalone Bundle > \"../dist/js/libs.js\" && uglifyjs \"../dist/js/libs.js\" > \"../dist/js/libs.min.js\" "
  },
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/RomanRanniew/tdsoft_aion.git"
  },
